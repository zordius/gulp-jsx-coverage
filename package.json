    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "coffee-react-transform": "5.0.0",
    "coffee-script": "1.12.3",
    "eslint": "3.14.1",
    "eslint-plugin-react": "6.9.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^3.0.1",
